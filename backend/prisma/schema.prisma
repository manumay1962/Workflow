generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  schema   = "public"
}


model User {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  password String
  username String?
}

model Workflow {
  id       String   @id
  name     String
  tags     String[]
  status   String
  owner    String
  runs     String[]
  schedule String
  nextRun  String
  isPublic Boolean  @default(false)
}
